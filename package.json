{
  "name": "nextjs-tailwind-landing-page",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "start:stag": "env-cmd -f .env.staging next dev",
    "build:dev": "env-cmd -f .env.development next build",
    "start:prod": "env-cmd -f .env.production next dev",
    "build:prod": "env-cmd -f .env.production next build",
    "build:stag": "env-cmd -f .env.staging next build",
    "export": "npm run build:prod && next export -o build",
    "export-stag": "npm run build:stag && next export -o build",
    "lint": "next lint"
  },
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@enzoferey/ethers-error-parser": "^0.2.3",
    "@hookform/resolvers": "^3.1.0",
    "@mui/material": "^5.13.0",
    "@web3modal/ethereum": "^2.3.7",
    "@web3modal/react": "^2.3.7",
    "ethers": "^5",
    "hoist-non-react-statics": "^3.3.2",
    "moment": "^2.29.4",
    "next": "12.2.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.43.9",
    "react-infinite-scroll-component": "^6.1.0",
    "react-modal": "^3.16.1",
    "react-slideshow-image": "^4.2.1",
    "react-toastify": "^9.1.2",
    "sass": "^1.62.1",
    "sharp": "^0.32.1",
    "wagmi": "^0.12.13",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/node": "18.6.3",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "autoprefixer": "^10.4.8",
    "env-cmd": "^10.1.0",
    "eslint": "^8.21.0",
    "eslint-config-next": "12.2.3",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "react-icons": "^4.4.0",
    "tailwindcss": "^3.1.7",
    "typescript": "4.7.4"
  }
}
